{
  "version": 2,
  "name": "textlint-web-api",
  "alias": [
    "textlint-api"
  ],
  "builds": [
    {
      "src": "src/**/*.ts",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/src/api/$1"
    }
  ]
}
